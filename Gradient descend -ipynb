      "source": [
        "def derivada(x):\n",
        "\n",
        "    x_deriv = 2*x \n",
        "    return x_deriv\n",
        "\n",
        "\n",
        "def gradient(x_start, precision, l_r):\n",
        "\n",
        "    x_list, y_list = [x_start], [function(x_start)]\n",
        "\n",
        "    while True:\n",
        "\n",
        "        d_x = - derivada(x_start)\n",
        "        x_start += (l_r * d_x)\n",
        "        x_list.append(x_start)\n",
        "        \n",
        "        #Inserindo o novo valor de y a lista \n",
        "        y_list.append(function(x_start))\n",
        "\n",
        "        # Definindo o criterio de parada \n",
        "        if abs(x_list[-1] - x_list[-2]) <= precision:\n",
        "            break\n",
        "\n",
        "    # Visualizando os resultados  \n",
        "\n",
        "    print (\"O mínimo local é: \"+ str(x_start))\n",
        "    print (\"Número de iterações: \" + str(len(x_list)))\n",
        "    \n",
        "    plt.subplot(1,2,2)\n",
        "    plt.scatter(x_list,y_list,c=\"g\")\n",
        "    plt.plot(x_list,y_list,c=\"g\")\n",
        "    plt.plot(x,function(x), c=\"r\")\n",
        "    plt.title(\"Descida do gradiente\")\n",
        "    plt.show()"
      ],
      "execution_count": 11,
      "outputs": []
    },
